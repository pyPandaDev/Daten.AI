# Gemini API Configuration
GEMINI_API_KEY=GEMINI_API_KEY=your_api_key_here,your_second_api_key_here,your_third_api_key_here
GEMINI_MODEL=gemini-2.5-flash

# Backend Configuration
# BACKEND_HOST: The IP address the server binds to (0.0.0.0 = all network interfaces)
BACKEND_HOST=0.0.0.0
# BACKEND_PORT: The port number where the API server runs
BACKEND_PORT=8000
# CORS_ORIGINS: Allowed origins for Cross-Origin Resource Sharing (comma-separated)
# This allows your frontend to communicate with the backend API
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# File Upload Configuration
MAX_FILE_SIZE_MB=50
ALLOWED_EXTENSIONS=csv,xlsx,xls,json,parquet

# Execution Configuration
EXECUTION_TIMEOUT_SECONDS=300
MAX_DATAFRAME_ROWS=100000
